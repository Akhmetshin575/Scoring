                                       Text="Стоимость:"></TextBlock>
                                <TextBox x:Name="textBoxRATPrice"
                                     Margin="0 10 0 10"
                                     Width="100"></TextBox>

                                <TextBlock x:Name="textBlockRATPartner"
                                       Margin="15 10 5 10"
                                       FontWeight="Medium"
                                       Text="Поставщик:"></TextBlock>
                                <ComboBox Name="comboBoxRATPartner"
                                      Margin="0 10 0 10"
                                      Width="570" MouseEnter="ComboBoxRATPartner_MouseEnter"></ComboBox>

                                <TextBlock x:Name="textBlockRATInvoiceNumber"
                                       Margin="10 10 5 10"
                                       FontWeight="Medium"
                                       Text="№ счета:"></TextBlock>
                                <TextBox x:Name="textBoxRATInvoiceNumber"
                                     Margin="0 10 0 10"
                                     Width="100"></TextBox>

                                <TextBlock x:Name="textBlockRATInvoiceDate"
                                       Margin="10 10 5 10"
                                       FontWeight="Medium"
                                       Text="Дата счета:"></TextBlock>
