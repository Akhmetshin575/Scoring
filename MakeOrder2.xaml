<Page x:Class="Scoring3.MakeOrder2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Scoring3"
      mc:Ignorable="d" 
      d:DesignHeight="1630" d:DesignWidth="1536"
      Title="MakeOrder2"
      Background="White">

    <ScrollViewer x:Name="scroll" VerticalScrollBarVisibility="Visible">
        <Grid ShowGridLines="False">
            <Grid.RowDefinitions>
                <RowDefinition Height="320"></RowDefinition>
                <RowDefinition Height="330"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <Grid x:Name="auto"
                  Grid.Row="0"
                  Grid.Column="0"
                  Grid.ColumnSpan="3">
                <WrapPanel
                    HorizontalAlignment="Center">
                    <GroupBox Header="Сведения об автомобиле:"
                              HorizontalAlignment="Stretch"
                              Margin="10 10 10 0"
                              FontWeight="Bold"
                              FontSize="16"
                              BorderBrush="Black"
                              BorderThickness="2"
                              Width="1500"
                          >
                        <WrapPanel Name="car">
                            <TextBlock x:Name="textBlockCity"
                                   Margin="0 10 10 0"
                                   Text="Город:"></TextBlock>
                            <ComboBox Name="comboBoxCity"
                                      Margin="0 10 0 0"
                                      Width="200"></ComboBox>
                            
                            <TextBlock x:Name="textBlockDealer"
                                   Margin="20 10 10 0"
                                   Text="Продавец:"></TextBlock>
                            <ComboBox Name="comboBoxDealer"
                                      Margin="0 10 0 0"
                                      Width="600" MouseEnter="ComboBoxDealer_MouseEnter"></ComboBox>

                            <TextBlock x:Name="textBlockBankDetails"
                                   Margin="20 10 10 0"
                                   Text="Расч. счет:"></TextBlock>
                            <ComboBox Name="comboBankDetails"
                                      Margin="0 10 0 0"
                                      Width="380" MouseEnter="ComboBankDetails_MouseEnter"></ComboBox>

                            <TextBlock x:Name="textBlockBrand"
                                   Margin="0 20 10 0"
                                   Text="Производитель:"></TextBlock>
                            <ComboBox Name="comboBoxBrand"
                                      Margin="0 20 0 0"
                                      Width="250"></ComboBox>

                            <TextBlock x:Name="textBlockModel"
                                   Margin="40 20 10 0"
                                   Text="Модель:"></TextBlock>
                            <ComboBox Name="comboBoxModel"
                                      Margin="0 20 0 0"
                                      Width="250" MouseEnter="ComboBoxModel_MouseEnter"></ComboBox>

                            <TextBlock x:Name="textBlockYear"
                                   Margin="40 20 10 0"
                                   Text="Год выпуска:"></TextBlock>
                            <TextBox x:Name="textBoxYear"
                                     Margin="0 20 0 0"
                                     Width="100" LostFocus="TextBoxYear_LostFocus"></TextBox>
                            

                            <TextBlock x:Name="textBlockNew"
                                   Margin="40 20 10 0"
                                   Text="Автомобиль новый?:"></TextBlock>
                            <RadioButton x:Name="radioButtonNewCar"
                             GroupName="Car"
                             Content="Да"
                             HorizontalAlignment="Left"
                             Margin="10,25,0,0"
                             VerticalAlignment="Top"
                             FontSize="14"
                             FontWeight="Bold"/>
                            <RadioButton x:Name="radioButtonUsedCar"
                            GroupName="Car"
                            Content="Нет"
                            HorizontalAlignment="Left"
                            Margin="10,25,0,0"
                            VerticalAlignment="Top"
                            FontSize="14"
                            FontWeight="Bold"/>
                            <TextBlock Width="100" Height="0"></TextBlock>

                            
                            <TextBlock x:Name="textBlockDocumentType"
                                   Margin="0 15 15 0"
                                   Text="Тип документа:"></TextBlock>
                            <ComboBox Name="comboBoxDocumentType"
                                      Margin="0 15 0 0"
                                      Width="210"></ComboBox>

                            <TextBlock x:Name="textBlockSeries"
                                   Margin="30 20 10 0"
                                   Text="Серия:"></TextBlock>
                            <TextBox x:Name="textBoxSeries"
                                     Margin="0 20 0 0"
                                     Width="100" LostFocus="TextBoxSeries_LostFocus"></TextBox>

                            <TextBlock x:Name="textBlockDocumentNumber"
                                   Margin="17 20 10 0"
                                   Text="Номер:"></TextBlock>
                            <TextBox x:Name="textBoxDocumentNumber"
                                     Margin="0 20 0 0"
                                     Width="150" LostFocus="TextBoxDocumentNumber_LostFocus"></TextBox>

                            <TextBlock x:Name="textBlockDocumentDate"
                                   Margin="17 20 10 0"
                                   Text="Дата выдачи:"></TextBlock>
                            <TextBox x:Name="textBoxDocumentDate"
                                     Margin="0 20 0 0"
                                     Width="100" LostFocus="TextBoxDocumentDate_LostFocus"></TextBox>

                            <TextBlock x:Name="textBlockNameFromDocument"
                                   Margin="17 20 10 0"
                                   Text="Название по ПТС:"></TextBlock>
                            <TextBox x:Name="textBoxNameFromDocument"
                                     Margin="0 20 0 0"
                                     Width="300"></TextBox>

                            <TextBlock x:Name="textBlockVIN"
                                   Margin="0 20 10 0"
                                   Text="VIN-номер:"></TextBlock>
                            <TextBox x:Name="textBoxVIN"
                                     Margin="0 20 0 0"
                                     Width="360" LostFocus="TextBoxVIN_LostFocus"></TextBox>
                            
                            <TextBlock x:Name="textBlockBody"
                                   Margin="50 20 10 0"
                                   Text="Номер кузова:"></TextBlock>
                            <TextBox x:Name="textBoxBody"
                                     Margin="0 20 0 0"
                                     Width="360"></TextBox>

                            <Button Name="copy"
                                    HorizontalAlignment="Left"
                                    FontWeight="Bold"
                                    FontSize="18"
                                    VerticalAlignment="Center"
                                    Height="30"
                                    Width="30"
                                    Margin="10 10 0 -10" Click="Copy_Click">
                                <StackPanel>
                                    <Image Source="C:\Users\XtremeStation\Desktop\Diplom\Scoring\copy.png"
                                       Width="20"
                                       Height="20" Stretch="Fill"></Image>
                                </StackPanel>
                            </Button>

                            <TextBlock x:Name="textBlockUVEOS"
                                   Margin="20 20 10 0"
                                   Text="УВЭОС:"></TextBlock>
                            <TextBox x:Name="textBoxUVEOS"
                                     Margin="0 20 0 0"
                                     Width="350"></TextBox>

                            <TextBlock x:Name="textBlockEngine"
                                   Margin="0 20 10 0"
                                   Text="№ двигателя:"></TextBlock>
                            <TextBox x:Name="textBoxEngine"
                                     Margin="0 20 0 0"
                                     Width="170"></TextBox>

                            <TextBlock x:Name="textBlockEngineType"
                                   Margin="25 20 10 0"
                                   Text="Тип двигателя:"></TextBlock>
                            <ComboBox Name="comboBoxEngineType"
                                      Margin="0 20 0 0"
                                      Width="150"></ComboBox>

                            <TextBlock x:Name="textBlockEcologicalClass"
                                   Margin="25 20 10 0"
                                   Text="Экологический класс:"></TextBlock>
                            <ComboBox Name="comboBoxEcologicalClass"
                                      Margin="0 20 0 0"
                                      Width="100"></ComboBox>

                            <TextBlock x:Name="textBlockEnginePower"
                                   Margin="25 20 10 0"
                                   Text="Мощность (кВт):"></TextBlock>
                            <TextBox x:Name="textBoxEnginePower"
                                     Margin="0 20 0 0"
                                     Width="110"></TextBox>

                            <TextBlock x:Name="textBlockMass"
                                   Margin="25 20 10 0"
                                   Text="Разрешённая масса:"></TextBlock>
                            <TextBox x:Name="textBoxMass"
                                     Margin="0 20 0 0"
                                     Width="110" LostFocus="TextBoxMass_LostFocus"></TextBox>

                            <TextBlock x:Name="textBlockColor"
                                   Margin="0 20 10 0"
                                   Text="Цвет:"></TextBlock>
                            <TextBox x:Name="textBoxColor"
                                     Margin="0 20 0 10"
                                     Width="200"></TextBox>

                            <TextBlock x:Name="textBlockPrice"
                                   Margin="25 20 10 0"
                                   Text="Стоимость автомобиля:"></TextBlock>
                            <TextBox x:Name="textBoxPrice"
                                     Margin="0 20 0 10"
                                     Width="150"></TextBox>

                            <TextBlock x:Name="textBlockDocumentForCredit"
                                   Margin="25 20 10 0"
                                   Text="Основание продажи:"></TextBlock>
                            <ComboBox Name="comboBoxDocumentForCredit"
                                      Margin="0 20 0 10"
                                      Width="142"></ComboBox>

                            <TextBlock x:Name="textBlockDocumentForCreditNumber"
                                   Margin="25 20 10 10"
                                   Text="№ документа"></TextBlock>
                            <TextBox x:Name="textBoxDocumentForCreditNumber"
                                     Margin="0 20 0 10"
                                     Width="100"></TextBox>

                            <TextBlock x:Name="textBlockDocumentForCreditDate"
                                   Margin="25 20 10 10"
                                   Text="Дата документа"></TextBlock>
                            <TextBox x:Name="textBoxDocumentForCreditDate"
                                     Margin="0 20 0 10"
                                     Width="100" GotFocus="TextBoxDocumentForCreditDate_GotFocus" LostFocus="TextBoxDocumentForCreditDate_LostFocus"></TextBox>


                        </WrapPanel>
                    </GroupBox>
                </WrapPanel>
            </Grid>


            <Grid x:Name="financialBlock"
                  Grid.Row="1"
                  Grid.Column="0"
                  Grid.ColumnSpan="3">
                <WrapPanel
                    HorizontalAlignment="Center">
                    <GroupBox Header="Выбор тарифа и услуг:"
                              HorizontalAlignment="Stretch"
                              Margin="10 10 10 0"
                              FontWeight="Bold"
                              FontSize="16"
                              BorderBrush="Black"
                              BorderThickness="2"
                              Width="1500">
                        <WrapPanel Name="services">
                            <TextBlock x:Name="textBlockPeriod"
                                   Margin="0 10 10 10"
                                   Text="Срок кредита:"></TextBlock>
                            <ComboBox Name="comboBoxPeriod"
                                      Margin="0 10 0 10"
                                      Width="50"></ComboBox>

                            <TextBlock x:Name="textBlockInitialPayment"
                                   Margin="25 10 10 10"
