                                <ComboBox Name="comboBoxKASKOPartner"
                                      Margin="0 10 0 10"
                                      Width="664" MouseEnter="ComboBoxKASKOPartner_MouseEnter"></ComboBox>

                                <TextBlock x:Name="textBlockKASKOPolicySeries"
                                       Margin="78 0 5 10"
                                       FontWeight="Medium"
                                       Text="Серия полиса:" Width="135"></TextBlock>
                                <TextBox x:Name="textBoxKASKOPolicySeries"
                                     Margin="0 0 0 10"
                                     Width="100"></TextBox>

                                <TextBlock x:Name="textBlockKASKOPolicyNumber"
                                           Width="115"
                                       Margin="10 0 5 10"
                                       FontWeight="Medium"
                                       Text="Номер полиса:"></TextBlock>
                                <TextBox x:Name="textBoxKASKOPolicyNumber"
                                     Margin="0 0 0 10"
                                     Width="100"></TextBox>

                                <TextBlock x:Name="textBlockKASKORegistartionDate"
                                       Margin="10 0 5 10"
                                       FontWeight="Medium"
                                       Text="Дата оформления полиса:"></TextBlock>
                                <TextBox x:Name="textBoxKASKORegistartionDate"
                                     Margin="0 0 0 10"
                                     Width="100" GotFocus="TextBoxKASKORegistartionDate_GotFocus" LostFocus="TextBoxKASKORegistartionDate_LostFocus"></TextBox>

                                <TextBlock x:Name="textBlockKASKOStartDate"
                                       Margin="10 0 5 10"
                                       FontWeight="Medium"
                                       Text="Дата начала действия:"></TextBlock>
