                                         HorizontalAlignment="Left"
                                         Margin="10,25,0,0"
                                         VerticalAlignment="Top"
                                         FontSize="14"
                                         FontWeight="Bold"/>
                            <TextBlock x:Name="textBlockSMS_Price"
                                   Margin="40 20 10 10"
                                   Text="(Сумма в кредит: )"
                                       Width="950"></TextBlock>

                            <TextBlock x:Name="textBlockPBU"
                                   Margin="0 10 10 10"
                                   Text="Пакет банковских услуг:"></TextBlock>
                            <RadioButton x:Name="radioButtonPBU_On"
                                         GroupName="PBU"
                                         Content="Подключить"
                                         HorizontalAlignment="Left"
                                         Margin="10,15,0,0"
                                         VerticalAlignment="Top"
                                         FontSize="14"
                                         FontWeight="Bold"/>
                            <RadioButton x:Name="radioButtonPBU_Off"
                                         GroupName="PBU"
                                         Content="Не подключать"
                                         HorizontalAlignment="Left"
                                         Margin="10,15,0,0"
                                         VerticalAlignment="Top"
                                         FontSize="14"
                                         FontWeight="Bold"/>
                            <TextBlock x:Name="textBlockPBU_Price"
                                   Margin="40 10 10 10"
                                   Text="(Сумма в кредит: бесплатно)"
                                       Width="950"></TextBlock>
                            
                            <WrapPanel x:Name="wpADVANCE">
                                <TextBlock x:Name="textBlockAdvance"
                                   Margin="0 10 10 10"
                                   Text="АДВАНС:"></TextBlock>
                                <TextBlock x:Name="textBlockAdvancePrice"
                                       Margin="10 10 5 10"
                                       FontWeight="Medium"
                                       Text="Стоимость:"></TextBlock>
                                <TextBox x:Name="textBoxAdvancePrice"
                                     Margin="0 10 0 10"
                                     Width="100"></TextBox>

                                <TextBlock x:Name="textBlockAdvancePartner"
                                       Margin="15 10 5 10"
                                       FontWeight="Medium"
                                       Text="Поставщик:"></TextBlock>
