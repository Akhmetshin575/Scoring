                              Margin="10 10 10 0"
                              FontWeight="Bold"
                              FontSize="16"
                              BorderBrush="Black"
                              BorderThickness="2"
                              Width="1500">
                        <StackPanel x:Name="insurances"
                                    Orientation="Vertical">
                            
                            <WrapPanel Name="wpKASKO">
                                <TextBlock x:Name="textBlockKASKO"
                                   Margin="0 10 10 10"
                                   Text="КАСКО:"></TextBlock>
                                <TextBlock x:Name="textBlockKASKOSum"
                                           Width="135"
                                       Margin="10 10 5 10"
                                       FontWeight="Medium"
                                       Text="Страховая сумма:"></TextBlock>
