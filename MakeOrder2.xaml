                                      Margin="0 10 0 10"
                                      Width="570" MouseEnter="ComboBoxAdvancePartner_MouseEnter"></ComboBox>

                                <TextBlock x:Name="textBlockAdvanceInvoiceNumber"
                                       Margin="10 10 5 10"
                                       FontWeight="Medium"
                                       Text="№ счета:"></TextBlock>
                                <TextBox x:Name="textBoxAdvanceInvoiceNumber"
                                     Margin="0 10 0 10"
                                     Width="100"></TextBox>

                                <TextBlock x:Name="textBlockInvoiceDate"
                                       Margin="10 10 5 10"
                                       FontWeight="Medium"
                                       Text="Дата счета:"></TextBlock>
                                <TextBox x:Name="textBoxInvoiceDate"
                                     Margin="0 10 0 10"
                                     Width="100" GotFocus="TextBoxInvoiceDate_GotFocus" LostFocus="TextBoxInvoiceDate_LostFocus"></TextBox>

                                <TextBlock x:Name="textBlockToCredit1"
                                       Margin="10 10 10 10"
                                       FontWeight="Medium"
                                       Text="В кредит?:"></TextBlock>
                                <CheckBox x:Name="radioButtonToCredit1_Yes"
                                         HorizontalAlignment="Left"
                                         Margin="0,15,0,0"
                                         VerticalAlignment="Top"
                                         FontSize="14"
                                         FontWeight="Medium"/>
                            </WrapPanel>
                                    
                            <WrapPanel x:Name="wpRING">
                                <TextBlock x:Name="textBlockRING"
                                   Margin="0 10 35 10"
                                   Text="РИНГ:"></TextBlock>
                                <TextBlock x:Name="textBlockRingPrice"
                                       Margin="10 10 5 10"
                                       FontWeight="Medium"
                                       Text="Стоимость:"></TextBlock>
                                <TextBox x:Name="textBoxRingPrice"
                                     Margin="0 10 0 10"
                                     Width="100"></TextBox>

                                <TextBlock x:Name="textBlockRingPartner"
                                       Margin="15 10 5 10"
                                       FontWeight="Medium"
                                       Text="Поставщик:"></TextBlock>
                                <ComboBox Name="comboBoxRingPartner"
                                      Margin="0 10 0 10"
                                      Width="570" MouseEnter="ComboBoxRingPartner_MouseEnter"></ComboBox>

                                <TextBlock x:Name="textBlockRingInvoiceNumber"
                                       Margin="10 10 5 10"
                                       FontWeight="Medium"
                                       Text="№ счета:"></TextBlock>
                                <TextBox x:Name="textBoxRingInvoiceNumber"
                                     Margin="0 10 0 10"
                                     Width="100"></TextBox>

                                <TextBlock x:Name="textBlockRingInvoiceDate"
                                       Margin="10 10 5 10"
                                       FontWeight="Medium"
                                       Text="Дата счета:"></TextBlock>
                                <TextBox x:Name="textBoxRingInvoiceDate"
                                     Margin="0 10 0 10"
                                     Width="100" GotFocus="TextBoxRingInvoiceDate_GotFocus" LostFocus="TextBoxRingInvoiceDate_LostFocus"></TextBox>

                                <TextBlock x:Name="textBlockToCredit2"
                                       Margin="10 10 10 10"
                                       FontWeight="Medium"
                                       Text="В кредит?:"></TextBlock>
                                <CheckBox x:Name="radioButtonToCredit2_Yes"
                                         HorizontalAlignment="Left"
                                         Margin="0,15,0,0"
                                         VerticalAlignment="Top"
                                         FontSize="14"
                                         FontWeight="Medium"/>
                            </WrapPanel>

                            <WrapPanel x:Name="wpRAT">
                                <TextBlock x:Name="textBlockRAT"
