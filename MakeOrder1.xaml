<Page x:Class="Scoring3.MakeOrder1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Scoring3"
      mc:Ignorable="d" 
      d:DesignHeight="1225" d:DesignWidth="1536"
      Title="MakeOrder1"
      Background="White"
      HorizontalAlignment="Stretch">
    <ScrollViewer x:Name="scroll" VerticalScrollBarVisibility="Visible">
        <Grid x:Name="main" ShowGridLines="False">
            <Grid.RowDefinitions>
                <RowDefinition x:Name="firstStroke" Height="480"></RowDefinition>
                <RowDefinition Height="130"></RowDefinition>
                <RowDefinition Height="150"></RowDefinition>
                <RowDefinition Height="150"></RowDefinition>
                <RowDefinition Height="230"></RowDefinition>
                <RowDefinition Height="70"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="450"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <Grid Name="scanPanel"
                  Grid.Row="0"
                  Grid.Column="0"
                  Background="LightGray"
                  HorizontalAlignment="Stretch"
                  VerticalAlignment="Stretch"
                  >
                
                <Canvas x:Name="canvas"
                        Margin="5"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        MouseMove="Canvas_MouseMove"
                        MouseDown="Canvas_MouseDown"
                        ClipToBounds="True"
                        Background="LightGoldenrodYellow"
                        >
                    <Image x:Name="scan"
                           MaxWidth="440"
                           MaxHeight="465"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           >
                    </Image>
                </Canvas>
                <Button x:Name="openFile"
                        Content="Открыть файл"
                        HorizontalAlignment="Center"
                        FontWeight="Bold"
                        FontSize="18"
                        VerticalAlignment="Center"
                        Height="50"
                        Width="250"
                        Margin="0 0 0 80" Click="OpenFile_Click"></Button>

                <Button x:Name="scanDocument"
                        Content="Сканировать документ"
                        HorizontalAlignment="Center"
                        FontWeight="Bold"
                        FontSize="18"
                        VerticalAlignment="Center"
                        Height="50"
                        Width="250"
                        Margin="0 0 0 -80" Click="ScanDocument_Click"></Button>
            </Grid>

            <Grid x:Name="scanPanelFloorButtons"
                  Background="LightGoldenrodYellow"
                  Grid.Row="1">
                <Button x:Name="ocrDocument"
                        Content="Распознать документ"
                        HorizontalAlignment="Center"
                        FontWeight="Bold"
                        FontSize="18"
                        VerticalAlignment="Top"
                        Height="50"
                        Width="250"
                        Margin="0 10 0 0"
                        Background="LightGreen" Click="OcrDocument_Click"></Button>

                <Button x:Name="turnLeft"
                        HorizontalAlignment="Left"
                        FontWeight="Bold"
                        FontSize="18"
                        VerticalAlignment="Center"
                        Height="50"
                        Width="80"
                        Margin="10 50 0 0" Click="TurnLeft_Click">
                    <StackPanel>
                        <Image Source="left.png"
                               Width="50"
                               Height="50"></Image>
                    </StackPanel>
                </Button>

                <Button x:Name="turnRight"
                        HorizontalAlignment="Center"
                        FontWeight="Bold"
                        FontSize="18"
                        VerticalAlignment="Center"
                        Height="50"
                        Width="80"
                        Margin="-170 50 0 0" Click="TurnRight_Click">
                    <StackPanel>
                        <Image Source="right.png"
                               Width="50"
                               Height="50"></Image>
                    </StackPanel>
                </Button>

                <Button x:Name="cut"
                        Content="Обрезать"
                        HorizontalAlignment="Center"
                        FontWeight="Bold"
                        FontSize="18"
                        VerticalAlignment="Center"
                        Height="50"
                        Width="120"
                        Margin="50 50 0 0" Click="Cut_Click"/>

                <Button x:Name="clear"
                        Content="Очистить"
                        HorizontalAlignment="Right"
                        FontWeight="Bold"
                        FontSize="18"
                        VerticalAlignment="Center"
                        Height="50"
                        Width="120"
                        Margin="0 50 10 0" Click="Clear_Click"></Button>
            </Grid>
            
            <TextBlock x:Name="textBlockNameOfPage"
                     Grid.Column="1"
                     Grid.ColumnSpan="2"
                     HorizontalAlignment="Stretch"
                     Height="40"
                     Margin="0 0 0 85"
                     Text="Ввод персональных данных клиента"
                     FontSize="25"
                     FontWeight="Bold"
                     TextDecorations="Underline"
                     VerticalAlignment="Top"
                     TextAlignment="Center" 
                     />
            <WrapPanel Name="aboutClient"
                Grid.Row="0"
                Grid.Column="1"
                Grid.ColumnSpan="2"
                >
                <TextBlock x:Name="textBlockSurname"
                     Grid.Column="1"
                     HorizontalAlignment="Left"
                     Height="23" Margin="0,65,0,0"
                     TextWrapping="Wrap"
                     Text="Фамилия: "
                     VerticalAlignment="Top"
                     FontWeight="Bold"
                     FontSize="14"/>
                <TextBox x:Name="textBoxSurname"
                     HorizontalAlignment="Left"
                     Height="23" Margin="0,65,0,0"
                     TextWrapping="NoWrap"
                     Text=""
                     VerticalAlignment="Top"
                     Width="140"
                     FontSize="14" LostFocus="TextBoxSurname_LostFocus"/>
                <TextBlock x:Name="textBlockName"
                    Grid.Column="1"
                    HorizontalAlignment="Left"
                    Height="23" Margin="25,65,0,0"
                    TextWrapping="Wrap"
                    Text="Имя: "
                    VerticalAlignment="Top"
                    FontWeight="Bold"
                    FontSize="14" RenderTransformOrigin="-0.73,0.322"/>
                <TextBox x:Name="textBoxName"
                    Grid.Column="1"
                    HorizontalAlignment="Left"
                    Height="23" Margin="0,65,0,0"
                    TextWrapping="NoWrap"
                    Text=""
                    VerticalAlignment="Top"
                    Width="140"
                    FontSize="14" LostFocus="TextBoxName_LostFocus"/>
                <TextBlock x:Name="textBlockPatronymic"
                    Grid.Column="1"
                    HorizontalAlignment="Left"
                    Height="23" Margin="25,65,0,0"
                    TextWrapping="Wrap"
                    Text="Отчество: "
                    VerticalAlignment="Top"
                    FontWeight="Bold"
                    FontSize="14" RenderTransformOrigin="-0.73,0.322"/>
                <TextBox x:Name="textBoxPatronymic"
                    Grid.Column="1"
                    HorizontalAlignment="Left"
                    Height="23" Margin="0,65,0,0"
                    TextWrapping="NoWrap"
                    Text=""
                    VerticalAlignment="Top"
                    Width="140"
                    Grid.ColumnSpan="2"
                    FontSize="14" LostFocus="TextBoxPatronymic_LostFocus"/>
                <TextBlock x:Name="textBlockBirthday"
                Grid.Column="2"
                HorizontalAlignment="Left"
                Height="23" Margin="25,65,0,0"
                TextWrapping="Wrap"
                Text="Дата рождения: "
                VerticalAlignment="Top"
                FontWeight="Bold"
                FontSize="14" RenderTransformOrigin="-0.73,0.322"/>
                <TextBox x:Name="textBoxBirthday"
                Grid.Column="2"
                HorizontalAlignment="Left"
                Height="23" Margin="0,65,0,0"
                TextWrapping="NoWrap"
                Text=""
                VerticalAlignment="Top"
                Width="140"
                FontSize="14" LostFocus="TextBoxBirthday_LostFocus"/>
                <TextBlock x:Name="textBlockGender"
                HorizontalAlignment="Left"
                Height="23" Margin="25,65,0,0"
                TextWrapping="Wrap"
                Text="Пол: "
                VerticalAlignment="Top"
                FontWeight="Bold"
                FontSize="14" RenderTransformOrigin="-0.73,0.322"/>
                <RadioButton x:Name="radioButtonMale"
                         GroupName="Gender"
                         Content="М"
                         Grid.Column="2"
                         HorizontalAlignment="Left"
                         Margin="0,67,0,0"
                         VerticalAlignment="Top"
                         FontSize="14"
                         FontWeight="Bold"/>
                <RadioButton x:Name="radioButtonFemale"
                GroupName="Gender"
                Content="Ж"
                Grid.Column="2"
                HorizontalAlignment="Left"
                Margin="0,67,0,0"
                VerticalAlignment="Top"
                FontSize="14"
                FontWeight="Bold"/>

                <GroupBox Header="Паспорт:"
                          HorizontalAlignment="Stretch"
                          Margin="0 20 0 0"
                          FontWeight="Bold"
                          FontSize="16"
                          BorderBrush="Black"
                          BorderThickness="2"
                          >
                    <WrapPanel Name="passport">
                        <TextBlock x:Name="textBlockPassportSeries"
                               Text="Серия: " FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxPassportSeries"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="140"
                             FontSize="14" LostFocus="TextBoxPassportSeries_LostFocus"/>

                        <TextBlock x:Name="textBlockPassportNumber"
                               Text="Номер: " Margin="25 0 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxPassportNumber"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="140"
                             FontSize="14" LostFocus="TextBoxPassportNumber_LostFocus"/>

                        <TextBlock x:Name="textBlockDate"
                               Text="Дата выдачи: " Margin="25 0 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxDate"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="140"
                             FontSize="14" LostFocus="TextBoxDate_LostFocus"/>

                        <TextBlock x:Name="textBlockCode"
                               Text="Код подразделения: " Margin="25 0 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxCode"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="140"
                             FontSize="14" LostFocus="TextBoxCode_LostFocus"/>

                        <TextBlock x:Name="textBlockDivision"
                               Text="Кем выдан: " Margin="0 10 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxDivision"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10 0 0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="970"
                             FontSize="14" LostFocus="TextBoxDivision_LostFocus"/>

                        <TextBlock x:Name="textBlockPlaceOfBirth"
                               Text="Место рождения: " Margin="0 10 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxPlaceOfBirth"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10 0 0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="930"
                             FontSize="14" LostFocus="TextBoxPlaceOfBirth_LostFocus"/>
                    </WrapPanel>
                </GroupBox>

                <GroupBox Header="Водительское удостоверение:"
                          HorizontalContentAlignment="Stretch"
                          Margin="0 20 0 0"
                          FontWeight="Bold"
                          FontSize="16"
                          BorderBrush="Black"
                          BorderThickness="2"
                          Width="1066"
                          >
                    <WrapPanel Name="DriverLicense"
                               HorizontalAlignment="Stretch">
                        <TextBlock x:Name="textBlockDriverLicenseSeries"
                               Text="Серия: " FontSize="14" Margin="0 10"></TextBlock>
                        <TextBox x:Name="textBoxDriverLicenseSeries"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="140"
                             FontSize="14" LostFocus="TextBoxDriverLicenseSeries_LostFocus"/>

                        <TextBlock x:Name="textBlockDriverLicenseNumber"
                               Text="Номер: " Margin="25 10 0 10" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxDriverLicenseNumber"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="140"
                             FontSize="14" LostFocus="TextBoxDriverLicenseNumber_LostFocus"/>

                        <TextBlock x:Name="textBlockDriverLicenseDate"
                               Text="Дата выдачи: " Margin="25 10 0 10" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxDriverLicenseDate"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="140"
                             FontSize="14" LostFocus="TextBoxDriverLicenseDate_LostFocus"/>
                    </WrapPanel>
                </GroupBox>

                <GroupBox Header="Адрес регистрации:"
                          HorizontalAlignment="Stretch"
                          Margin="0 20 0 0"
                          FontWeight="Bold"
                          FontSize="16"
                          BorderBrush="Black"
                          BorderThickness="2"
                          Width="1066"
                          >
                    <WrapPanel Name="registration">
                        <TextBlock x:Name="textBlockRegistrationIndex"
                               Text="Индекс: " FontSize="14" Margin="0 10"></TextBlock>
                        <TextBox x:Name="textBoxRegistrationIndex"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="100"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockRegistrationRegion"
                               Text="Регион: " Margin="25 10 0 10" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxRegistrationRegion"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="170"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockRegistrationDistrict"
                               Text="Район: " Margin="25 10 0 10" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxRegistrationDistrict"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="170"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockRegistrationLocality"
                               Text="Населенный пункт: " Margin="25 10 0 10" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxRegistrationLocality"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="240"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockRegistrationSubordinateLocality"
                               Text="Подчиненный пункт: " FontSize="14" Margin="0 0"></TextBlock>
                        <TextBox x:Name="textBoxRegistrationSubordinateLocality"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="260"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockRegistrationStreet"
                               Text="Улица: " Margin="25 0 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxRegistrationStreet"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="260"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockRegistrationHome"
                               Text="Дом: " Margin="25 0 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxRegistrationHome"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="75"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockRegistrationApartment"
                               Text="Квартира: " Margin="25 0 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxRegistrationApartment"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="75"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockRegistrationDate"
                               Text="Дата регистрации: " Margin="0 10 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxRegistrationDate"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10 0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="120"
                             FontSize="14" LostFocus="TextBoxRegistrationDate_LostFocus"/>

                        <TextBlock x:Name="textBlockAdressMatch"
                        HorizontalAlignment="Left"
                        Height="23" Margin="50,10,0,0"
                        TextWrapping="Wrap"
                        Text="Фактическое место жительства совпадает с адресом регистрации?: "
                        VerticalAlignment="Top"
                        FontWeight="Bold"
                        FontSize="14" RenderTransformOrigin="-0.73,0.322"/>
                        <RadioButton x:Name="radioButtonAdressMatchYes"
                         GroupName="Adress"
                         Content="Да"
                         HorizontalAlignment="Left"
                         Margin="10,14,0,0"
                         VerticalAlignment="Top"
                         FontSize="14"
                         FontWeight="Bold" Checked="RadioButtonAdressMatchYes_Checked"/>
                        <RadioButton x:Name="radioButtonMatchNo"
                        GroupName="Adress"
                        Content="Нет"
                        HorizontalAlignment="Left"
                        Margin="10,14,0,0"
                        VerticalAlignment="Top"
                        FontSize="14"
                        FontWeight="Bold" Checked="RadioButtonMatchNo_Checked"/>
                    </WrapPanel>
                </GroupBox>
            </WrapPanel>


            <WrapPanel Name="factLivingBlock"
                Grid.Row="1"
                Grid.Column="1"
                Grid.ColumnSpan="2">
                <GroupBox Header="Фактическое место жительства:"
                          HorizontalAlignment="Stretch"
                          VerticalAlignment="Top"
                          Margin="0 0 0 20"
                          FontWeight="Bold"
                          FontSize="16"
                          BorderBrush="Black"
                          BorderThickness="2"
                          Width="1066"
                          >
                    <WrapPanel x:Name="factLiving"
                               Height="95">
                        <TextBlock x:Name="textBlockFactIndex"
                               Text="Индекс: " FontSize="14" Margin="0 10"></TextBlock>
                        <TextBox x:Name="textBoxFactIndex"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="100"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockFactRegion"
                               Text="Регион: " Margin="25 10 0 10" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxFactRegion"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="170"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockFactDistrict"
                               Text="Район: " Margin="25 10 0 10" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxFactDistrict"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="170"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockFactLocality"
                               Text="Населенный пункт: " Margin="25 10 0 10" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxFactLocality"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="240"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockFactSubordinateLocality"
                               Text="Подчиненный пункт: " FontSize="14" Margin="0 0 0 0"></TextBlock>
                        <TextBox x:Name="textBoxFactSubordinateLocality"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="260"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockFactStreet"
                               Text="Улица: " Margin="25 0 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxFactStreet"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="260"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockFactHome"
                               Text="Дом: " Margin="25 0 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxFactHome"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="75"
                             FontSize="14"/>

                        <TextBlock x:Name="textBlockFactApartment"
                               Text="Квартира: " Margin="25 0 0 0" FontSize="14"></TextBlock>
                        <TextBox x:Name="textBoxFactApartment"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="0 0"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="75"
                             FontSize="14"/>

                        
                    </WrapPanel>
                </GroupBox>
            </WrapPanel>


            <GroupBox
                Grid.Row="2"
                Grid.Column="1"
                Header="Мобильный телефон:"
                HorizontalAlignment="Stretch"
                Margin="0 0 5 0"
                FontWeight="Bold"
                FontSize="16"
                BorderBrush="Black"
                BorderThickness="2">
                <WrapPanel Name="mobilePhone">
                    <TextBlock x:Name="textBlockMobilePhone"
                           Text="Номер: "
                           FontSize="14"
                           Margin="0 10">
                    </TextBlock>
                    <TextBox x:Name="textBoxMobilePhone"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="10 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="150"
                             FontSize="14" LostFocus="TextBoxMobilePhone_LostFocus"/>

                    <TextBlock x:Name="textBlockMobilePhoneDate"
                           Text="Дата прозвона: "
                           FontSize="14"
                           Margin="20 10 0 10">
                    </TextBlock>
                    <TextBox x:Name="textBoxMobilePhoneDate"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="10 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="140"
                             FontSize="14" LostFocus="TextBoxMobilePhoneDate_LostFocus" GotFocus="TextBoxMobilePhoneDate_GotFocus"/>

                    <TextBlock x:Name="textBlockMobileResponder"
                           Text="Отвечающий: "
                           FontSize="14"
                           Margin="0 10 0 10">
                    </TextBlock>
                    <TextBox x:Name="textBoxMobilePhoneResponder"
                             HorizontalAlignment="Stretch"
                             Height="23"
                             Margin="10 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="395"
                             FontSize="14" GotFocus="TextBoxMobilePhoneResponder_GotFocus"/>

                    <TextBlock x:Name="textBlockMobileResult"
                           Text="Результат прозвона: "
                           FontSize="14"
                           Margin="0 0 0 10">
                    </TextBlock>
                    <ComboBox x:Name="comboBoxMobilePhoneResult"
                             HorizontalAlignment="Stretch"
                             Height="23"
                             Margin="10 0 10 10"
                             Text=""
                             VerticalAlignment="Top"
                             Width="352"
                             FontSize="14"/>
                </WrapPanel>
            </GroupBox>

            <GroupBox
                Grid.Row="3"
                Grid.Column="1"
                Header="Рабочий телефон:"
                HorizontalAlignment="Stretch"
                Margin="0 0 5 0"
                FontWeight="Bold"
                FontSize="16"
                BorderBrush="Black"
                BorderThickness="2">
                <WrapPanel Name="workPhone">
                    <TextBlock x:Name="textBlockWorkPhone"
                           Text="Номер: "
                           FontSize="14"
                           Margin="0 10">
                    </TextBlock>
                    <TextBox x:Name="textBoxWorkPhone"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="10 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="150"
                             FontSize="14" LostFocus="TextBoxWorkPhone_LostFocus"/>

                    <TextBlock x:Name="textBlockWorkPhoneDate"
                           Text="Дата прозвона: "
                           FontSize="14"
                           Margin="20 10 0 10">
                    </TextBlock>
                    <TextBox x:Name="textBoxWorkPhoneDate"
                             HorizontalAlignment="Left"
                             Height="23"
                             Margin="10 10"
                             TextWrapping="NoWrap"
                             Text=""
                             VerticalAlignment="Top"
                             Width="140"
                             FontSize="14" LostFocus="TextBoxWorkPhoneDate_LostFocus" GotFocus="TextBoxWorkPhoneDate_GotFocus"/>

                    <TextBlock x:Name="textBlockWorkResponder"
                           Text="Отвечающий: "
                           FontSize="14"
                           Margin="0 10 0 10">
